{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"nikhil133/checkpoint-client",
            "host":"client",
            "esential":false,
            "memory":128
        },
        {
            "name":"server",
            "image":"nikhil133/checkpoint-server",
            "host":"api",
            "esential":false,
            "memory":128
        },
        {
            "name":"worker",
            "image":"nikhil133/checkpoint-worker",
            "host":"worker",
            "esential":false,
            "memory":128
        },
        {
            "name":"nginx",
            "image":"nikhil133/checkpoint-nginx",
            "host":"nginx",
            "esential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"],
            "memory":128
        }
    ]
} 


 

